//Array Reduce Method
let arr3=[1,3,1,5,2]
const reduce_func=(h1,h2)=>{
  return h1+h2
}
let newarr3=arr3.reduce(reduce_func)
console.log(newarr3)
//reduce returns value
//map and filter returns array
